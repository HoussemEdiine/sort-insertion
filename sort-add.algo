ALGORITHM  Insertion sort
VAR
      c,v,i,n: INTEGER;
    array : ARRAY_OF INTEGER[n];
BEGIN
    Read(n);
    FOR i FROM 0 TO array.length-1 STEP 1  DO
      v := array[i]
      c := i
      WHILE (c>0 AND array[c-1]>value) DO
          array[c]:=array[c-1]
          c:=c-1
      END_WHILE
      array[c]:= value
    END_FOR
    Write(array);

END